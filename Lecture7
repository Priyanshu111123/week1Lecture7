#include <iostream>
using namespace std;
int main()
{
    int password;
    cout<<"Enter the password: "<<endl;
    cin>>password;
    while(password<999999)
    {
        cout<<"The password does not meet the required conditions";
        cin>>password;
    }
    return 0;
}
